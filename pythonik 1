#napisz funkcje, która zwraca iloczyn dowolnej ilosci podanych liczb jako argument

# def func_3(*args):
#     b = 1
#     for i in args:
#         b *= i
#     return b
# print(func_3(5,2,7))

# napisz funkcje, która wylicza ilosc zerowych i nie zerowych liczb calkowitych podanej jako parametr

def policz_liczby_w_calkowitej(liczba):
    # Inicjalizujemy liczniki
    zerowe = 0
    niezerowe = 0

    # Obsługa liczby ujemnej
    liczba = abs(liczba)

    # Przechodzimy przez każdą cyfrę liczby
    while liczba > 0:
        cyfra = liczba % 10  # Pobieramy ostatnią cyfrę liczby
        if cyfra == 0:
            zerowe += 1
        else:
            niezerowe += 1
        liczba //= 10  # Usuwamy ostatnią cyfrę liczby

    return zerowe, niezerowe

# Przykład użycia
liczba = 1049320
zerowe, niezerowe = policz_liczby_w_calkowitej(liczba)
print("Liczba zerowych:", zerowe)
print("Liczba niezerowych:", niezerowe)

