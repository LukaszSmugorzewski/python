lista = [10, 20, 25, 35, 40]

if all(i % 2 == 0 for i in lista):
    print("Wszystkie parzyste")

if any(i % 2 == 0 for i in lista):
    print("chociaż 1 parzysta")

    for i in enumerate(lista):
        print(i[0] + 1,'-', i[1])

