n = int(input("wpisz liczbe naturalną n: "))
if n > 0:
    suma = 0.0
    for x in range (n):
        y = float(input(f"podaj {x+1} liczbe rzeczywistą: "))
        if y % 3 == 0 and y % 5 != 0:
            print("liczba", y, "jest podzielna przez 3 a nie podzielna przez 5")

